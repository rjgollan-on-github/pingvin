package pingvin

Conserved_Quantities :: enum {
    mass,
    xmom,
    ymom,
    zmom,
    energy,
}

Primitive_Quantities :: enum {
    rho, // density, kg/m^3
    p, // pressure, Pa
    T, // temperature, K
    e, // internal energy, J/kg
    ke, // kinetic energy, J/kg
    xvel, // x-velocity component, m/s
    yvel, // y-velocity component, m/s
    zvel, // z-velocity component, m/s
}




